  <artifactId>synergy</artifactId>
  <name>Synergy</name>
  <version>0.0.4-SNAPSHOT</version>
  <description>Basic tools and server messaging plugin</description>
  <build>
    <sourceDirectory>src/main/java</sourceDirectory>
    <resources>
      <resource>
        <targetPath>.</targetPath>
        <filtering>true</filtering>
        <directory>${basedir}/src/main/java/</directory>
        <includes>
          <include>plugin.yml</include>
          <include>bungee.yml</include>
          <include>config.yml</include>
          <include>locales.yml</include>
        </includes>
      </resource>
    </resources>
    <finalName>Synergy</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.2.1</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
        <configuration />
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.1</version>
        <configuration>
          <source>8</source>
          <target>8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
